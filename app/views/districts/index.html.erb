<p id="notice"><%= notice %></p>

<h1>Districts</h1>

<table>
  <thead>
    <tr>
      <th>Oblast</th>
      <th>Name</th>
      <th>Soate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
        <td><%= district.oblast_id %></td>
        <td><%= district.name %></td>
        <td><%= district.soate %></td>
        <td><%= link_to 'Show', district %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
